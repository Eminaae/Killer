package ba.bitcamp.week05.day4.vjezbe;

public class Main {

	public static void main(String[] args) {
		PersonalComputer pc = new PersonalComputer();
		pc.setOs("Android");
		pc.setPrice(1500);
		pc.setRam(8);
		System.out.println(pc.getOs());
		System.out.println(pc.getPrice());
		System.out.println(pc.getRam());
		
		PortableComputer p = new PortableComputer();
		p.setDisplaySize(17);
		p.setHasWiFi(true);
		p.setWeight(5);
		System.out.println(p.getDisplaySize());
		System.out.println(p.getHasWiFi());
		System.out.println(p.getWeight());
		
		StationaryComputer sc = new StationaryComputer();
		sc.setSpeed(250);
		sc.setPower(15);
		sc.setHdCapacity(1);
		System.out.println(sc.getSpeed());
		System.out.println(sc.getPower());
		System.out.println(sc.getHdCapacity());
		
		DesktopComputer dc = new DesktopComputer();
		dc.getIsOverclock();
		dc.getRamSlots();
		dc.getHasOpticalDrive();
		
		Laptop l = new Laptop();
		l.getHasBluetooth();
		l.getBatteryCells();
		l.getHasNumKeyboard();
		
		PocketPC pp = new PocketPC();
		pp.getIsTouch();
		pp.getMicroSim();
		pp.getSim();
		
		
	}
}
