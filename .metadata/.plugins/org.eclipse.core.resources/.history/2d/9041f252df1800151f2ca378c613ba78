package ba.bitcamp.week06.tasks;

public class Rectangle implements Shape, Drawable {
	public int width;
	public int height;
	@Override
	public void drawOnConsole() {
		System.out.println("****");
		System.out.println("****");
		System.out.println("****");
		
	}
	@Override
	public int Circumference() {
		// TODO Auto-generated method stub
		return 0;
	}
	@Override
	public int gerArea() {
		
		return 2*width + 2*height;
	}
	

}
